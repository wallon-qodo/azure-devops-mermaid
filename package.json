{
  "name": "ado-mermaid-viewer",
  "version": "2.1.4",
  "description": "Professional Mermaid diagram renderer for Azure DevOps with GitHub Flavored Markdown support",
  "private": true,
  "dependencies": {
    "azure-devops-extension-api": "^4.265.0",
    "azure-devops-extension-sdk": "^4.1.0",
    "marked": "^16.3.0",
    "mermaid": "^11.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@types/jest": "^29.5.14",
    "babel-jest": "^30.2.0",
    "html-webpack-plugin": "^5.6.4",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "raw-loader": "^4.0.2",
    "webpack": "^5.102.0",
